{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "fstrainingazureadf"
		},
		"FileServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FileServer1'"
		},
		"Fstrainingblobls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Fstrainingblobls'"
		},
		"fstrainingAzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'fstrainingAzureSqlDatabase'"
		},
		"FileServer1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "./"
		},
		"FileServer1_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "rahulkumar"
		},
		"storagetrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/619fbad9-63aa-4e57-8f23-9eaf2a224aeb/resourceGroups/fstrainingresourcegrp/providers/Microsoft.Storage/storageAccounts/fstrainingstrgact"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "fstrainingazureadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Fstrainingblobls_connectionString": {
						"value": "[parameters('Fstrainingblobls_connectionString')]"
					},
					"fstrainingAzureSqlDatabase_connectionString": {
						"value": "[parameters('fstrainingAzureSqlDatabase_connectionString')]"
					},
					"FileServer1_password": {
						"value": "[parameters('FileServer1_password')]"
					},
					"FileServer1_properties_typeProperties_host": {
						"value": "[parameters('FileServer1_properties_typeProperties_host')]"
					},
					"FileServer1_properties_typeProperties_userId": {
						"value": "[parameters('FileServer1_properties_typeProperties_userId')]"
					},
					"storagetrigger_properties_typeProperties_scope": {
						"value": "[parameters('storagetrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}