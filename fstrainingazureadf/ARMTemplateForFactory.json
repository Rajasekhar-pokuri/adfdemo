										"timeout": "0.12:00:00",
										"retry": 0,
										"retryIntervalInSeconds": 30,
										"secureOutput": false,
										"secureInput": false
									},
									"userProperties": [],
									"typeProperties": {
										"url": "https://prod-85.eastus.logic.azure.com:443/workflows/2a4e20fa940c47eb949e58545034bfcd/triggers/manual/paths/invoke?api-version=2016-10-01&sp=%2Ftriggers%2Fmanual%2Frun&sv=1.0&sig=S8Ny5m8acw6WTfKi596bCKspM-BjlUto66v0k9x_BYs",
										"method": "POST",
